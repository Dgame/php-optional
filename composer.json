{
    "name": "dgame/php-optional",
    "description": "php optional",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/php-optional",
    "keywords": [
        "Option",
        "Optional",
        "Maybe",
        "NullObject",
        "Some",
        "None"
    ],
    "authors": [
        {
            "name": "Randy Schuett ",
            "email": "rswhite4@gmail.com",
            "homepage": "https://github.com/php-optional"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.11.12"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Dgame\\Optional\\": "src/",
            "Dgame\\Optional\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse src --level=7"
        ]
    }
}
